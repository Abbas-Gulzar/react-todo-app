 [todos, setTodos] = useState(getInitialTodos())\n  const handleChange = id => {\n    setTodos(prevState =>\n      prevState.map(todo => {\n        if (todo.id === id) {\n          return {\n            ...todo,\n            completed: !todo.completed,\n          }\n        }\n        return todo\n      })\n    )\n  }\n\n  const delTodo = id => {\n    setTodos([\n      ...todos.filter(todo => {\n        return todo.id !== id\n      }),\n    ])\n  }\n\n  const addTodoItem = title => {\n    const newTodo = {\n      id: uuidv4(),\n      title: title,\n      completed: false,\n    }\n    setTodos([...todos, newTodo])\n  }\n\n  const setUpdate = (updatedTitle, id) => {\n    setTodos(\n      todos.map(todo => {\n        if (todo.id === id) {\n          todo.title = updatedTitle\n        }\n        return todo\n      })\n    )\n  }\n\n  function getInitialTodos() {\n    // getting stored items\n    const temp = localStorage.getItem(\"todos\")\n    const savedTodos = JSON.parse(temp)\n    return savedTodos || []\n  }\n\n  useEffect(() => {\n    // storing todos items\n    const temp = JSON.stringify(todos)\n    localStorage.setItem(\"todos\", temp)\n  }, [todos])\n\n  return (\n    <switch>\n      <Navbar />\n      <Route exact path=\"/\">\n        <div className=\"container\">\n          <div className=\"inner\">\n            <Header />\n            <InputTodo addTodoProps={addTodoItem} />\n            <TodosList\n              todos={todos}\n              handleChangeProps={handleChange}\n              deleteTodoProps={delTodo}\n              setUpdate={setUpdate}\n            />\n          </div>\n        </div>\n      </Route>\n      <Route path=\"/about\">\n        <About />\n      </Route>\n      <Route exact path=\"/NotMatch\">\n        <NotMatch />\n      </Route>\n    </switch>\n  )\n}\n\nexport default TodoContainer","/home/abbas/React/react-tutorial-update/src/functionBased/components/Header.js",[],"/home/abbas/React/react-tutorial-update/src/functionBased/components/InputTodo.js",[],"/home/abbas/React/react-tutorial-update/src/functionBased/components/TodosList.js",[],"/home/abbas/React/react-tutorial-update/src/functionBased/components/TodoItem.js",[],"/home/abbas/React/react-tutorial-update/src/functionBased/pages/NotMatch.js",[],"/home/abbas/React/react-tutorial-update/src/functionBased/pages/About.js",[],"/home/abbas/React/react-tutorial-update/src/functionBased/components/Navbar.js",[],"/home/abbas/React/react-tutorial-update/src/functionBased/pages/SinglePage.js",[],["50","51"],{"ruleId":"52","severity":1,"message":"53","line":5,"column":27,"nodeType":"54","messageId":"55","endLine":5,"endColumn":33},{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","replacedBy":"59"},{"ruleId":"52","severity":1,"message":"60","line":6,"column":17,"nodeType":"54","messageId":"55","endLine":6,"endColumn":23},{"ruleId":"56","replacedBy":"61"},{"ruleId":"58","replacedBy":"62"},"no-unused-vars","'Router' is defined but never used.","Identifier","unusedVar","no-native-reassign",["63"],"no-negated-in-lhs",["64"],"'Switch' is defined but never used.",["63"],["64"],"no-global-assign","no-unsafe-negation"]